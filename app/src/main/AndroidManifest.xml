<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nexgo.apiv3demo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.nexgo.apiv3demoSDK206.NexgoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.nexgo.apiv3demoSDK206.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nexgo.apiv3demoSDK206.PinpadActivity"></activity>
        <activity android:name="com.nexgo.apiv3demoSDK206.PrinterActivity"></activity>
        <activity android:name="com.nexgo.apiv3demoSDK206.EmvActivity"></activity>
        <activity android:name="com.nexgo.apiv3demoSDK206.PinpadCustomLayoutActivity"></activity>
    </application>

</manifest>
